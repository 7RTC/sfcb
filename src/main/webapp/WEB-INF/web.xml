<?xml version="1.0" encoding="utf-8"?>
<web-app
        version="2.5"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>br.com.javamagazine.sfcb.SignGuestbookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teste</servlet-name>
        <servlet-class>br.com.javamagazine.sfcb.FacebookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Foto</servlet-name>
        <servlet-class>br.com.javamagazine.sfcb.FotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageProxy</servlet-name>
        <servlet-class>br.com.javamagazine.sfcb.ImageProxy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ColagemServlet</servlet-name>
        <servlet-class>br.com.javamagazine.sfcb.ColagemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teste</servlet-name>
        <url-pattern>/Teste</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Foto</servlet-name>
        <url-pattern>/Fotos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageProxy</servlet-name>
        <url-pattern>/Fotos/ImageProxy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ColagemServlet</servlet-name>
        <url-pattern>/Colagem</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>collage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
